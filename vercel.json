{
  "version": 2,
  "builds": [{ "src": "**", "use": "@vercel/static" }],
  "routes": [
    { "src": "/(css|js)/(.*)", "dest": "/$1/$2" },
    { "src": "/(.*)\\.(json|png|jpg|svg|ico)", "dest": "/$1.$2" },
    { "src": "/pergunta-[0-9]+", "dest": "/index.html" },
    { "src": "/resultado", "dest": "/index.html" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
